package vaibhav;

public class Recursive_Fibonacci {
	
	public static int fib(int n) {
		if(n == 0 || n==1) {
			return n;
		}
		int fibn1 = fib(n-1);
		int fibn2 = fib(n-2);
		
		int fn = fib(n-1) + fib(n-2);
		
		return fn;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n = 15;
		System.out.println(fib(n));
	}

}
